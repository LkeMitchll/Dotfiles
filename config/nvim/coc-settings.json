{
  "coc.preferences.extensionUpdateCheck": "daily",
  "emmet.includeLanguages": {
    "typescriptreact": ["css", "html"]
  },
  "snippets.extends": {
    "typescriptreact": ["css", "html", "javascript"],
    "typescript": ["javascript"]
  },
  "stylelintplus.autoFixOnFormat": true,
  "list.insertMappings": {
    "<C-c>": "do:exit",
    "<C-v>": "action:vsplit",
    "<C-x>": "action:split",
    "<C-t>": "action:tabe",
    "<C-z>": "do:toggle"
  },
  "list.normalMappings": {
    "<C-c>": "do:exit"
  },
  "list.source.grep.command": "ag",
  "list.source.files.args": [
    "-f",
    "-g",
    ".",
    "--nocolor",
    "--hidden",
    "--ignore",
    ".git"
  ],
  "suggest.enablePreview": true,
  "suggest.noselect": false
}
