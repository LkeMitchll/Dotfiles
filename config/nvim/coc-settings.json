{
  "suggest.enablePreview": true,
  "emmet.includeLanguages": {
    "typescriptreact": "html",
    "eruby": "html",
    "htmldjango": "html"
  },
  "list.insertMappings": {
    "<C-c>": "do:exit",
    "<C-v>": "action:vsplit",
    "<C-x>": "action:split",
    "<C-t>": "action:tabe",
    "<C-z>": "do:toggle"
  },
  "list.source.files.command": "rg",
  "list.source.files.args": ["--files", "--hidden", "-i"],
  "snippets.extends": {
    "typescriptreact": ["typescript", "html", "javascript"],
    "typescript": ["javascript"]
  },
  "stylelintplus.autoFixOnFormat": true
}
